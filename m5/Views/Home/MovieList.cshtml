@{
    ViewData["Title"] = "Movie List";
}

@model List<FormResponse>

<table class="table table-bordered">
    <thead class="font-weight-bold">
        <tr>
            <td>Title</td>
            <td>Year</td>
            <td>Director</td>
            <td>Rating</td>
            <td>Edited</td>
            <td>LentTo</td>
            <td>Notes</td>
        </tr>
    </thead>

    <tbody>
        <!--for each entry create table row, td= table domain-->
        @foreach (var x in Model)
            {
        <tr>
            <td>@x.Title</td>
            <td>@x.Year</td>
            <td>@x.Director</td>
            <td>@x.Rating</td>
            <td>@x.Edited</td>
            <td>@x.LentTo</td>
            <td>@x.Notes</td>

            <td><a asp-controller="Home" asp-action="Edit" asp-route-applicationid="@x.FormID">Edit</a></td>
            <td><a asp-controller="Home" asp-action="Delete" asp-route-applicationid="@x.FormID">Delete</a></td>


        </tr>
            }
    </tbody>
</table>